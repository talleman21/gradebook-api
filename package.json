{
  "dependencies": {
    "@prisma/client": "^3.2.1",
    "@types/http-errors": "^1.8.1",
    "@types/jest": "^27.0.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "ts-node": "^10.3.0",
    "dotenv-cli": "^4.0.0",
    "joi": "^17.4.2",
    "prisma": "^3.2.1"
  },
  "scripts": {
    "start": "dotenv ts-node server.ts",
    "dev": "cross-env DB_PORT=5432 && yarn test-db-up && sleep 3 && dotenv ts-node-dev server.ts",
    "test-db-up": "docker-compose -p gradebook -f ./docker/docker-compose.test.yaml up -d && sleep 2 && yarn db-migrate",
    "db-migrate": "npx prisma migrate dev",
    "test": "jest --coverage",
    "format": "yarn run eslint --fix src && yarn prettier --write src",
    "prepare": "husky install"
  },
  "name": "gradebook-api",
  "version": "1.0.0",
  "description": "gradebook app api",
  "main": "server.ts",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^16.10.9",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "commitlint": "^13.2.1",
    "cross-env": "^7.0.3",    
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.2.5",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "^11.2.6",
    "nodemon": "^2.0.13",
    "prettier": "2.4.1",
    "ts-jest": "^27.0.7",    
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "author": "Thomas Alleman",
  "license": "ISC"
}
